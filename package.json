{
  "name": "aerolab-welcome",
  "version": "1.0.0",
  "description": "A step-by-step guide to setting up your computer and your accounts on Aerolab's team.",
  "main": "src/main.js",
  "scripts": {
    "build": "electron-packager . 'Aerolab Welcome' --out=build --app-version=$npm_package_version --prune --asar --overwrite --platform=darwin --arch=x64 --icon='src/images/aerolab.icns' --ignore='build' --ignore='cache' --ignore='.git' --ignore='gulpfile.js' --ignore='README.md'",
    "dev": "node ./node_modules/.bin/gulp dev"
  },
  "keywords": [
    "electron",
    "aerolab",
    "welcome"
  ],
  "author": "Aerolab <hey@aerolab.co> (https://aerolab.co)",
  "license": "MIT",
  "dependencies": {
    "shelljs": "^0.7.8",
    "slug": "^0.9.1",
    "slugify": "^1.2.1",
    "sudo-prompt": "^7.1.1"
  },
  "devDependencies": {
    "electron": "^1.7.9",
    "electron-packager": "^9.1.0",
    "gulp": "^3.9.1",
    "gulp-electron": "^0.1.3",
    "gulp-livereload": "^3.8.1",
    "rimraf": "^2.6.2",
    "semver-regex": "^1.0.0"
  }
}
